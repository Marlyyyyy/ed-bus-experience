<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#008a32" />
            <gradient android:angle="-90"  android:startColor="#008a32" android:endColor="#008a32" />
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#009f3a" />
            <gradient  android:angle="-90"  android:startColor="#56ab5a" android:endColor="#56ab5a"  />
        </shape>
    </item>
</selector>